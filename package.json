  "description": "Mutopia - Premium Pet Grooming Landing Page",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{vue,ts,js,css}\"",
    "format:check": "prettier --check \"src/**/*.{vue,ts,js,css}\"",
    "lint": "eslint src --ext .vue,.ts,.js --fix",
    "lint:check": "eslint src --ext .vue,.ts,.js",
    "prepare": "husky"
